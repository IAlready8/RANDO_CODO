logger.error(â€˜WebSocket error:â€™, error);
});
});

function broadcast(data) {
const message = JSON.stringify(data);
clients.forEach(client => {
if (client.readyState === WebSocket.OPEN) {
try {
client.send(message);
} catch (error) {
logger.error(â€˜WebSocket broadcast error:â€™, error);
}
}
});
}

