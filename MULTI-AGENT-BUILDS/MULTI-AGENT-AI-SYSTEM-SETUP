# Create .eslintrc.json
cat > .eslintrc.json << 'EOF'
{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "react/no-unescaped-entities": "off"
  }
}
EOF

# Create .prettierrc
cat > .prettierrc << 'EOF'
{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
EOF

# FULL directory structure creation BEFORE ANY FILE
mkdir -p src/app/api/chat
mkdir -p "src/app/api/chat/[sessionId]"
mkdir -p src/app/api/llm
mkdir -p src/app/api/llm/providers
mkdir -p src/app/api/session
mkdir -p src/app/chat
mkdir -p src/app/compare
mkdir -p src/app/pipeline
mkdir -p src/components
mkdir -p src/lib
mkdir -p src/types
mkdir -p prisma
mkdir -p public

# Create Prisma schema
cat > prisma/schema.prisma << 'EOF'
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String?
  userId    String?
  messages  Message[]
  pipelines Pipeline[]
}

model Message {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  content    String
  role       String   // user, assistant, system
  model      String   // gpt-4, claude, gemini, etc.
  tokens     Int?
  latency    Int?     // milliseconds
}

model Pipeline {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  name       String
  steps      String   // JSON array of pipeline steps
  results    String?  // JSON results
  status     String   @default("pending") // pending, running, completed, failed
}

model LLMProvider {
  id         String   @id @default(uuid())
  name       String   @unique
  endpoint   String
  apiKey     String
  models     String   // JSON array of available models
  enabled    Boolean  @default(true)
  createdAt  DateTime @default(now())
}
EOF

