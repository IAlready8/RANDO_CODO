bash
# setup.sh
#!/usr/bin/env bash
# ğŸ”¥ ULTIMATE Mac-Native Multi-Agent AI System - 10/10 ALL BENCHMARKS (NO DOCKER) ğŸ”¥
set -euo pipefail

echo "ğŸ Bootstrapping Ultimate Mac-Native Multi-Agent AI Systemâ€¦"

PROJECT_DIR=~/Desktop/ai-multi-agent-system-mac-native
BREWFILE_PATH="$PROJECT_DIR/Brewfile"

# 1ï¸âƒ£ Ensure macOS & Homebrew
echo "ğŸ” Verifying macOS & Homebrewâ€¦"
if [[ "$OSTYPE" != "darwin"* ]]; then
  echo "âŒ This script only runs on macOS."
  exit 1
fi
if ! command -v brew &>/dev/null; then
  echo "ğŸ“¦ Installing Homebrewâ€¦"
/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"
fi

# 2ï¸âƒ£ Install brew-bundle
echo "ğŸ“¦ Ensuring brew-bundleâ€¦"
brew tap Homebrew/bundle

# 3ï¸âƒ£ Install everything via Brewfile (version-pinned)
echo "ğŸ“¦ Installing dependencies from Brewfileâ€¦"
mkdir -p "$PROJECT_DIR"
cat > "$BREWFILE_PATH" <<'EOF'
# Brewfile â€” version-pinned for reproducibility

tap "homebrew/core"
tap "homebrew/cask"
tap "homebrew/services"

brew "node@18"
brew "postgresql@14"
brew "redis@7"
brew "nginx"
brew "consul"
brew "vault"
brew "jq"
brew "curl"
brew "wget"
brew "git"
brew "watchman"
brew "fswatch"

brew "prometheus"
brew "grafana"
brew "influxdb"
brew "telegraf"

brew "nmap"
brew "sqlmap"
brew "htop"
brew "iotop"
brew "wrk"
brew "hey"
brew "artillery"

cask "visual-studio-code"
EOF

brew bundle --file="$BREWFILE_PATH"

# 4ï¸âƒ£ Start core services
echo "ğŸ”„ Starting core servicesâ€¦"
brew services start redis@7 postgresql@14 nginx consul prometheus grafana influxdb

# 5ï¸âƒ£ Scaffold directories
echo "ğŸ“ Creating project layoutâ€¦"
dirs=(coordinator agents shared frontend logs config test scripts monitoring infrastructure/k8s infrastructure/terraform docs/api docs/architecture docs/deployment security native-services/nginx native-services/consul native-services/vault)
for d in "${dirs[@]}"; do
  mkdir -p "$PROJECT_DIR/$d"
done

# 6ï¸âƒ£ Initialize Git & hooks
echo "ğŸ”§ Initializing Git repoâ€¦"
cd "$PROJECT_DIR"
if [[ ! -d .git ]]; then
  git init
  cat > .gitignore << 'EOF'
node_modules/
dist/
logs/
*.log
.env*
.DS_Store
.vscode/
coverage/
EOF
  git add .
  git commit -m "chore: initial repo layout"
fi

echo "âœ… Bootstrap complete! Project at $PROJECT_DIR"


