export interface Message {
  id: string
  content: string
  role: 'user' | 'assistant' | 'system'
  model: string
  createdAt: Date
  tokens?: number
  latency?: number
}

export interface Session {
  id: string
  title?: string
  createdAt: Date
  updatedAt: Date
  messages: Message[]
}

export interface LLMProvider {
  id: string
  name: string
  endpoint: string
  models: string[]
  enabled: boolean
}

export interface PipelineStep {
  model: string
  prompt: string
  temperature?: number
  maxTokens?: number
}

export interface Pipeline {
  id: string
  name: string
  steps: PipelineStep[]
  status: 'pending' | 'running' | 'completed' | 'failed'
  results?: any
}
