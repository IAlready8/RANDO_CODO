    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": ["darwin"],
  "cpu": ["arm64", "x64"],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
