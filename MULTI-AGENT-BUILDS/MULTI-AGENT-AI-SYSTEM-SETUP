ğŸ› ï¸ scripts/bootstrap-all.sh
#!/usr/bin/env bash
set -euo pipefail

# â€”â€”â€” CONFIGURATION â€”â€”â€”
ROOT="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)"
BREWFILE="$ROOT/Brewfile"

echo "ğŸš€ Bootstrapping EVERYTHING into $ROOTâ€¦"

# 1ï¸âƒ£ macOS & Homebrew check
if [[ "$OSTYPE" != "darwin"* ]]; then
  echo "âŒ Mac only."
  exit 1
fi
if ! command -v brew &>/dev/null; then
  echo "ğŸ“¦ Installing Homebrewâ€¦"
  /bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"
fi
brew tap Homebrew/bundle

# 2ï¸âƒ£ Write Brewfile (version-pinned deps)
cat > "$BREWFILE" << 'EOF'
tap "homebrew/core"
tap "homebrew/cask"
tap "homebrew/services"

brew "node@18"
brew "postgresql@14"
brew "redis@7"
brew "nginx"
brew "consul"
brew "vault"
brew "jq"
brew "curl"
brew "wget"
brew "git"
brew "watchman"
brew "fswatch"

brew "prometheus"
brew "grafana"
brew "influxdb"
brew "telegraf"

brew "nmap"
brew "sqlmap"
brew "htop"
brew "iotop"
brew "wrk"
brew "hey"
brew "artillery"

cask "visual-studio-code"
EOF

echo "ğŸ“¦ Installing Homebrew depsâ€¦"
brew bundle --file="$BREWFILE"

# 3ï¸âƒ£ Start core services
echo "ğŸ”„ Starting core servicesâ€¦"
brew services start redis@7 postgresql@14 nginx consul prometheus grafana influxdb

# 4ï¸âƒ£ Scaffold directories
echo "ğŸ“ Creating foldersâ€¦"
for d in \
  infrastructure/terraform infrastructure/k8s \
  monitoring alertmanager native-services/nginx native-services/consul native-services/vault \
  scripts src/coordinator test/performance security frontend; do
  mkdir -p "$ROOT/$d"
done

# 5ï¸âƒ£ Initialize Git
cd "$ROOT"
if [[ ! -d .git ]]; then
  git init
  cat > .gitignore << 'EOF'
node_modules/
dist/
logs/
*.log
.env*
.DS_Store
.vscode/
coverage/
EOF
  git add .
  git commit -m "chore: initial monorepo layout"
fi

# 6ï¸âƒ£ Write all project files
echo "âœï¸  Writing configuration & codeâ€¦"

# â”€â”€â”€ Docker & Compose â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > "$ROOT/Dockerfile" << 'EOF'
# Dockerfile â€” cross-platform fallback
FROM node:18-alpine
RUN apk add --no-cache redis postgresql-client nginx consul vault curl jq
WORKDIR /app
COPY package.json package-lock.json ./
RUN npm ci --production
COPY dist ./dist
COPY native-services/nginx/nginx.conf /etc/nginx/nginx.conf
EXPOSE 3000 8080 80
CMD ["sh","-c","\ 
  redis-server --daemonize yes && \ 
  pg_ctl start -D /var/lib/postgresql/data && \ 
  nginx && \ 
  consul agent -dev -config-dir=/app/native-services/consul & \ 
  vault server -dev -dev-root-token-id=dev-token & \ 
  node dist/coordinator/server.js"]
EOF

cat > "$ROOT/docker-compose.yml" << 'EOF'
version: "3.8"
services:
  coordinator:
    build: .
    ports:
      - "3000:3000"
      - "8080:8080"
    environment:
      - NODE_ENV=production
      - PORT=3000
      - DATABASE_URL=postgresql://postgres:password@postgres:5432/ai_agents
      - REDIS_URL=redis://redis:6379
      - CONSUL_HOST=consul
      - CONSUL_PORT=8500
      - VAULT_ADDR=http://vault:8200
      - VAULT_TOKEN=dev-token
    depends_on:
      - redis
      - postgres
      - consul
      - vault
  redis:
    image: redis:7-alpine
    ports: ["6379:6379"]
  postgres:
    image: postgres:14-alpine
    environment:
      - POSTGRES_PASSWORD=password
      - POSTGRES_DB=ai_agents
    ports: ["5432:5432"]
    volumes:
      - pgdata:/var/lib/postgresql/data
  consul:
    image: consul:1.15
    command: "agent -dev -client=0.0.0.0"
    ports: ["8500:8500","8600:8600/udp"]
  vault:
    image: vault:1.14
    command: "server -dev -dev-root-token-id=dev-token"
    ports: ["8200:8200"]
  prometheus:
    image: prom/prometheus:v2.44
    volumes:
      - ./monitoring/prometheus.yml:/etc/prometheus/prometheus.yml
      - ./monitoring/prometheus-rules.yml:/etc/prometheus/prometheus-rules.yml
    ports: ["9090:9090"]
  grafana:
    image: grafana/grafana:10.0
    environment:
      - GF_SECURITY_ADMIN_USER=admin
      - GF_SECURITY_ADMIN_PASSWORD=admin
    ports: ["3001:3001"]
    volumes:
      - grafana-data:/var/lib/grafana
volumes:
  pgdata:
  grafana-data:
EOF

# â”€â”€â”€ GitHub Actions â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
mkdir -p .github/workflows
cat > .github/workflows/ci.yml << 'EOF'
name: CI
on:
  push:    { branches: [ main ] }
  pull_request: { branches: [ main ] }
jobs:
  test-build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-node@v4
        with: { node-version: 18 }
      - run: npm ci
      - run: npm run lint:fix
      - run: npm run build
      - run: npm run test:unit
      - run: npm run test:integration
      - run: npm run test:e2e
      - run: |
          npm run test:performance
          npm run test:security
EOF

cat > .github/workflows/release.yml << 'EOF'
name: Release
on:
  push:
    tags: ['v*.*.*']
jobs:
  release:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-node@v4
        with: { node-version: 18 }
      - run: |
          npm ci
          npm run build
      - run: npx semantic-release
        env:
          GITHUB_TOKEN: ${{ secrets.GITHUB_TOKEN }}
          NPM_TOKEN: ${{ secrets.NPM_TOKEN }}
EOF

# â”€â”€â”€ Terraform (AWS ECS) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > infrastructure/terraform/main.tf << 'EOF'
terraform {
  required_providers {
    aws = { source="hashicorp/aws", version=">= 4.0" }
  }
  required_version = ">= 1.3"
}
provider "aws" { region = var.aws_region }
variable "aws_region" { type=string; default="us-east-1" }
resource "aws_ecs_cluster" "ai_agents" { name = "ai-multi-agent-cluster" }
# (plus ecs_task_definition, ecs_service stubsâ€¦)
EOF

# â”€â”€â”€ Kubernetes â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > infrastructure/k8s/deployment.yaml << 'EOF'
apiVersion: apps/v1
kind: Deployment
metadata: { name: ai-coordinator }
spec:
  replicas: 3
  selector: { matchLabels: { app: ai-coordinator } }
  template:
    metadata: { labels: { app: ai-coordinator } }
    spec:
      containers:
      - name: coordinator
        image: your-docker-repo/ai-coordinator:latest
        ports: [{ containerPort: 3000 }]
        env:
        - name: NODE_ENV; value: "production"
        - name: PORT;      value: "3000"
---
apiVersion: v1
kind: Service
metadata: { name: ai-coordinator }
spec:
  selector: { app: ai-coordinator }
  ports: [{ protocol: TCP, port: 80, targetPort: 3000 }]
  type: ClusterIP
EOF

# â”€â”€â”€ Monitoring â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > monitoring/prometheus.yml << 'EOF'
global:
  scrape_interval: 15s
scrape_configs:
  - job_name: 'ai-multi-agent'
    static_configs: [{ targets: ['localhost:3000'], metrics_path: /metrics }]
  # redis, postgres, node-exporterâ€¦
EOF

cat > monitoring/prometheus-rules.yml << 'EOF'
groups:
- name: ai-agent-alerts
  rules:
    - alert: HighErrorRate
      expr: rate(task_error_total[5m]) / rate(task_success_total[5m]) > 0.05
      for: 10m
      labels: { severity: critical }
      annotations:
        summary: "High error rate"
        description: "Error rate = {{ $value }}"
    - alert: CoordinatorHighCPU
      expr: process_cpu_usage > 80
      for: 5m
      labels: { severity: warning }
    - alert: CoordinatorDown
      expr: up{job="ai-multi-agent"} == 0
      for: 2m
      labels: { severity: critical }
EOF

cat > monitoring/alertmanager/config.yml << 'EOF'
global:
  resolve_timeout: 5m
route:
  receiver: pagerduty
receivers:
  - name: pagerduty
    pagerduty_configs:
      - service_key: "<YOUR_PAGERDUTY_KEY>"
        severity: '{{ if eq .CommonLabels.severity "critical" }}critical{{ else }}warning{{ end }}'
EOF

cat > monitoring/grafana.ini << 'EOF'
[server]
http_addr = 127.0.0.1
http_port = 3001
domain    = localhost
[database]
type = sqlite3
path = monitoring/grafana.db
[security]
admin_user     = admin
admin_password = admin
secret_key     = mac-native-secret
[analytics]
reporting_enabled = false
[log]
mode  = file
level = info
EOF

# â”€â”€â”€ Native-services Nginx â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > native-services/nginx/nginx.conf << 'EOF'
worker_processes auto;
pid logs/nginx.pid;
events { worker_connections 1024; use kqueue; }
http {
  include /usr/local/etc/nginx/mime.types;
  default_type application/octet-stream;
  access_log logs/access.log main;
  error_log  logs/error.log warn;
  sendfile on; gzip on; tcp_nodelay on;
  upstream nodejs_backend { server 127.0.0.1:3000; }
  server {
    listen 80; server_name localhost;
    location /api/ { proxy_pass http://nodejs_backend; }
    location / {      proxy_pass http://nodejs_backend; }
  }
  server { listen 3001; server_name localhost; location / { proxy_pass http://127.0.0.1:3000; } }
}
EOF

# â”€â”€â”€ Scripts â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
cat > scripts/start-native-services.js << 'EOF'
