4. scripts/check-services.js:

#!/usr/bin/env node
import { execSync } from 'child_process';
import axios from 'axios';

console.log('ğŸ” Checking Mac-native service statusâ€¦\n');

const services = [
  { name: 'Redis', check: async () => {
      try { execSync('redis-cli ping', { stdio: 'ignore' }); return { status: 'âœ… Running', port: 6379 }; }
      catch { return { status: 'âŒ Not running', port: 6379 }; }
    }
  },
  { name: 'PostgreSQL', check: async () => {
      try { execSync('pg_isready', { stdio: 'ignore' }); return { status: 'âœ… Running', port: 5432 }; }
      catch { return { status: 'âŒ Not running', port: 5432 }; }
    }
  },
  { name: 'Consul', check: async () => {
      try { await axios.get('http://localhost:8500/v1/status/leader',{timeout:2000}); return { status:'âœ… Running', port:8500 }; }
      catch { return { status:'âŒ Not running', port:8500 }; }
    }
  },
  { name: 'Vault', check: async () => {
      try { await axios.get('http://localhost:8200/v1/sys/health',{timeout:2000}); return { status:'âœ… Running', port:8200 }; }
      catch { return { status:'âŒ Not running', port:8200 }; }
    }
  },
  { name: 'Prometheus', check: async () => {
      try { await axios.get('http://localhost:9090/-/healthy',{timeout:2000}); return { status:'âœ… Running', port:9090 }; }
      catch { return { status:'âŒ Not running', port:9090 }; }
    }
  },
  { name: 'Grafana', check: async () => {
      try { await axios.get('http://localhost:3001/api/health',{timeout:2000}); return { status:'âœ… Running', port:3001 }; }
      catch { return { status:'âŒ Not running', port:3001 }; }
    }
  }
];

(async () => {
  for (const svc of services) {
    try {
      const r = await svc.check();
      console.log(${svc.name.padEnd(12)} ${r.status.padEnd(15)} Port: ${r.port});
    } catch (e) {
      console.log(${svc.name.padEnd(12)} âŒ Error checking     ${e.message});
    }
  }
})();


