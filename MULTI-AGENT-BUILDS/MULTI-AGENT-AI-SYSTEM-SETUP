# Dockerfile â€” cross-platform fallback
FROM node:18-alpine

# Install dependencies for native components
RUN apk add --no-cache redis postgresql-client nginx consul vault curl jq

WORKDIR /app
COPY package.json package-lock.json ./
RUN npm ci --production

COPY dist ./dist
COPY native-services/nginx/nginx.conf /etc/nginx/nginx.conf

EXPOSE 3000 8080 80

CMD ["sh", "-c", "\
  redis-server --daemonize yes && \
  pg_ctl start -D /var/lib/postgresql/data && \
  nginx && \
  consul agent -dev -config-dir=/app/native-services/consul & \
  vault server -dev -dev-root-token-id=dev-token & \
  node dist/coordinator/server.js \
"]
# docker-compose.yml
version: "3.8"
services:
  coordinator:
    build: .
    ports:
      - "3000:3000"
      - "8080:8080"
    environment:
      - NODE_ENV=production
      - PORT=3000
      - DATABASE_URL=postgresql://postgres:password@postgres:5432/ai_agents
      - REDIS_URL=redis://redis:6379
      - CONSUL_HOST=consul
      - CONSUL_PORT=8500
      - VAULT_ADDR=http://vault:8200
      - VAULT_TOKEN=dev-token
    depends_on:
      - redis
      - postgres
      - consul
      - vault

  redis:
    image: redis:7-alpine
    ports: ["6379:6379"]

  postgres:
    image: postgres:14-alpine
    environment:
      - POSTGRES_PASSWORD=password
      - POSTGRES_DB=ai_agents
    ports: ["5432:5432"]
    volumes:
      - pgdata:/var/lib/postgresql/data

  consul:
    image: consul:1.15
    command: "agent -dev -client=0.0.0.0"
    ports: ["8500:8500","8600:8600/udp"]

  vault:
    image: vault:1.14
    command: "server -dev -dev-root-token-id=dev-token"
    ports: ["8200:8200"]

  prometheus:
    image: prom/prometheus:v2.44
    volumes:
      - ./monitoring/prometheus.yml:/etc/prometheus/prometheus.yml
      - ./monitoring/prometheus-rules.yml:/etc/prometheus/prometheus-rules.yml
    ports: ["9090:9090"]

  grafana:
    image: grafana/grafana:10.0
    environment:
      - GF_SECURITY_ADMIN_USER=admin
      - GF_SECURITY_ADMIN_PASSWORD=admin
    ports: ["3001:3001"]
    volumes:
      - grafana-data:/var/lib/grafana

volumes:
  pgdata:
  grafana-data:
# .github/workflows/ci.yml
name: CI

on:
  push:
    branches: [ main ]
  pull_request:
    branches: [ main ]

jobs:
  test-build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - name: Setup Node.js
        uses: actions/setup-node@v4
        with:
          node-version: 18
      - name: Install Dependencies
        run: npm ci
      - name: Lint
        run: npm run lint:fix
      - name: Build
        run: npm run build
      - name: Unit Tests
        run: npm run test:unit
      - name: Integration Tests
        run: npm run test:integration
      - name: E2E Tests
        run: npm run test:e2e
      - name: Performance & Security
        run: |
          npm run test:performance
          npm run test:security
# .github/workflows/release.yml
name: Release

on:
  push:
    tags:
      - 'v*.*.*'

jobs:
  release:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - name: Setup Node.js
        uses: actions/setup-node@v4
        with:
          node-version: 18
      - name: Install & Build
        run: |
          npm ci
          npm run build
      - name: Run Semantic Release
        env:
          GITHUB_TOKEN: ${{ secrets.GITHUB_TOKEN }}
          NPM_TOKEN: ${{ secrets.NPM_TOKEN }}
        run: npx semantic-release
# infrastructure/terraform/main.tf
terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = ">= 4.0"
    }
  }
  required_version = ">= 1.3"
}

provider "aws" {
  region = var.aws_region
}

variable "aws_region" {
  type    = string
  default = "us-east-1"
}

resource "aws_ecs_cluster" "ai_agents" {
  name = "ai-multi-agent-cluster"
}

resource "aws_ecs_task_definition" "coordinator" {
  family                   = "ai-coordinator"
  network_mode             = "awsvpc"
  requires_compatibilities = ["FARGATE"]
  cpu                      = "512"
  memory                   = "1024"
  execution_role_arn       = aws_iam_role.ecs_task_exec.arn
  container_definitions    = jsonencode([
    {
      name      = "coordinator"
      image     = "your-docker-repo/ai-coordinator:latest"
      essential = true
      portMappings = [
        { containerPort = 3000, protocol = "tcp"},
        { containerPort = 8080, protocol = "tcp"}
      ]
      environment = [
        { name = "NODE_ENV", value = "production" },
        { name = "PORT", value = "3000" }
      ]
    }
  ])
}

resource "aws_ecs_service" "coordinator" {
  name            = "ai-coordinator"
  cluster         = aws_ecs_cluster.ai_agents.id
  task_definition = aws_ecs_task_definition.coordinator.arn
  desired_count   = 2
  launch_type     = "FARGATE"
  network_configuration {
    subnets         = var.subnet_ids
    assign_public_ip = true
    security_groups = [aws_security_group.ecs.id]
  }
}

# (Define aws_iam_role, security_group, subnet_ids elsewhere)
# infrastructure/k8s/deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: ai-coordinator
  labels:
    app: ai-coordinator
spec:
  replicas: 3
  selector:
    matchLabels:
      app: ai-coordinator
  template:
    metadata:
      labels:
        app: ai-coordinator
    spec:
      containers:
      - name: coordinator
        image: your-docker-repo/ai-coordinator:latest
        ports:
        - containerPort: 3000
        env:
        - name: NODE_ENV
          value: "production"
        - name: PORT
          value: "3000"
---
apiVersion: v1
kind: Service
metadata:
  name: ai-coordinator
spec:
  selector:
    app: ai-coordinator
  ports:
  - protocol: TCP
    port: 80
    targetPort: 3000
  type: ClusterIP
# monitoring/prometheus-rules.yml
groups:
  - name: ai-agent-alerts
    rules:
      - alert: HighErrorRate
        expr: rate(task_error_total[5m]) / rate(task_success_total[5m]) > 0.05
        for: 10m
        labels:
          severity: critical
        annotations:
          summary: "High error rate (>5%) in the last 5m"
          description: "Error rate = {{ $value }}"

      - alert: CoordinatorHighCPU
        expr: process_cpu_usage > 80
        for: 5m
        labels:
          severity: warning
        annotations:
          summary: "Coordinator CPU usage >80%"
          description: "CPU usage at {{ $value }}%"

      - alert: CoordinatorDown
        expr: up{job="ai-multi-agent"} == 0
        for: 2m
        labels:
          severity: critical
        annotations:
          summary: "Coordinator service down"
# monitoring/alertmanager/config.yml
global:
  resolve_timeout: 5m
route:
  receiver: pagerduty
receivers:
  - name: pagerduty
    pagerduty_configs:
      - service_key: "<YOUR_PAGERDUTY_INTEGRATION_KEY>"
        severity: '{{ if eq .CommonLabels.severity "critical" }}critical{{ else }}warning{{ end }}'
