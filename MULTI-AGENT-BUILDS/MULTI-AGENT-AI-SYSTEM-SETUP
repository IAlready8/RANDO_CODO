// Initialize APM
const apmAgent = apm.start({
serviceName: â€˜ai-multi-agent-coordinatorâ€™,
environment: process.env.NODE_ENV || â€˜developmentâ€™,
distributedTracing: true,
captureBody: â€˜errorsâ€™,
captureErrorLogStackTraces: â€˜alwaysâ€™
});

// Enhanced logging with correlation IDs
const logger = pino({
level: process.env.LOG_LEVEL || â€˜infoâ€™,
transport: {
target: â€˜pino-prettyâ€™,
options: {
colorize: true,
translateTime: â€˜SYS:standardâ€™,
