    static_configs:
      - targets: ['localhost:3000']
        metrics_path: /metrics
        scrape_interval: 5s
  - job_name: 'node-exporter'
    static_configs:
      - targets: ['localhost:9100']
  - job_name: 'redis'
    static_configs:
      - targets: ['localhost:6379']
  - job_name: 'postgres'
    static_configs:
      - targets: ['localhost:5432']

rule_files:
  - "rules/*.yml"

alerting:
  alertmanagers:
    - static_configs:
        - targets:
            - localhost:9093


