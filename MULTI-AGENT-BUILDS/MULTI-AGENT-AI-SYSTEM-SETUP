});
});

// Graceful shutdown
process.on(â€˜SIGTERMâ€™, () => {
logMessage(â€˜infoâ€™, â€˜SIGTERM received, shutting down gracefullyâ€™);

wss.close(() => {
logMessage(â€˜infoâ€™, â€˜WebSocket server closedâ€™);
});

