# Create Prisma schema
cat > prisma/schema.prisma << 'EOF'
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String?
  userId    String?
  messages  Message[]
  pipelines Pipeline[]
}

model Message {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  content    String
  role       String   // user, assistant, system
  model      String   // gpt-4, claude, gemini, etc.
  tokens     Int?
  latency    Int?     // milliseconds
}

model Pipeline {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  name       String
  steps      String   // JSON array of pipeline steps
  results    String?  // JSON results
  status     String   @default("pending") // pending, running, completed, failed
}

model LLMProvider {
  id         String   @id @default(uuid())
  name       String   @unique
  endpoint   String
  apiKey     String
  models     String   // JSON array of available models
  enabled    Boolean  @default(true)
  createdAt  DateTime @default(now())
}
EOF

# Create src/lib/prisma.ts
cat > src/lib/prisma.ts << 'EOF'
import { PrismaClient } from '@prisma/client'

const globalForPrisma = globalThis as unknown as {
  prisma: PrismaClient | undefined
}

export const prisma = globalForPrisma.prisma ?? new PrismaClient()

if (process.env.NODE_ENV !== 'production') globalForPrisma.prisma = prisma

export default prisma
EOF

# Create src/types/index.ts
cat > src/types/index.ts << 'EOF'
export interface Message {
  id: string
  content: string
  role: 'user' | 'assistant' | 'system'
  model: string
  createdAt: Date
  tokens?: number
  latency?: number
}

export interface Session {
  id: string
  title?: string
  createdAt: Date
  updatedAt: Date
  messages: Message[]
}

export interface LLMProvider {
  id: string
  name: string
  endpoint: string
  models: string[]
  enabled: boolean
}

export interface PipelineStep {
  model: string
  prompt: string
  temperature?: number
  maxTokens?: number
}

export interface Pipeline {
  id: string
  name: string
  steps: PipelineStep[]
  status: 'pending' | 'running' | 'completed' | 'failed'
  results?: any
}
EOF

# Create src/app/layout.tsx
cat > src/app/layout.tsx << 'EOF'
import type { Metadata } from 'next'
import { Inter } from 'next/font/google'
import './globals.css'

const inter = Inter({ subsets: ['latin'] })

export const metadata: Metadata = {
  title: 'Multi-LLM Chat Platform',
  description: 'Orchestrate multiple LLMs in one platform',
}

export default function RootLayout({
  children,
}: {
  children: React.ReactNode
}) {
  return (
    <html lang="en" className="dark">
      <body className={inter.className}>{children}</body>
    </html>
  )
}
EOF

# Create src/app/globals.css
cat > src/app/globals.css << 'EOF'
@tailwind base;
@tailwind components;
@tailwind utilities;

:root {
  --foreground-rgb: 255, 255, 255;
  --background-start-rgb: 0, 0, 0;
  --background-end-rgb: 0, 0, 0;
}

body {
  color: rgb(var(--foreground-rgb));
  background: linear-gradient(
      to bottom,
      transparent,
      rgb(var(--background-end-rgb))
    )
    rgb(var(--background-start-rgb));
}

@layer base {
  * {
    @apply border-border;
  }
  body {
    @apply bg-background text-foreground;
  }
}
EOF

