    "pm2": "^5.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "prom-client": "^15.1.0",
    "node-vault": "^0.10.2",
    "consul": "^0.40.0",
    "pidusage": "^3.0.2",
    "systeminformation": "^5.21.15",
    "opossum": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
