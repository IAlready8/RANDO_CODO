export const SecurityPolicy = {
// Content Security Policy
csp: {
directives: {
defaultSrc: [â€â€˜selfâ€™â€],
styleSrc: [â€â€˜selfâ€™â€, â€œâ€˜unsafe-inlineâ€™â€],
