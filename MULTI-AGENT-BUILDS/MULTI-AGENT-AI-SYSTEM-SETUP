},

// Password policy
password: {
minLength: 8,
requireUppercase: true,
requireLowercase: true,
requireNumbers: true,
requireSpecialChars: true
},

// API security
api: {
maxRequestSize: â€˜10mbâ€™,
allowedMethods: [â€˜GETâ€™, â€˜POSTâ€™, â€˜PUTâ€™, â€˜DELETEâ€™, â€˜OPTIONSâ€™],
allowedHeaders: [
â€˜Content-Typeâ€™,
â€˜Authorizationâ€™,
â€˜X-Requested-Withâ€™,
â€˜X-Correlation-IDâ€™
]
