README.md
# Multi-LLM Chat Platform

A powerful platform for orchestrating multiple Large Language Models (LLMs) in a single interface.

## Features
- **Multi-LLM Chat**: Converse with different LLMs (GPT-4, Claude, Gemini, etc.)
- **Model Comparison**: Compare responses from multiple models side-by-side
- **Pipeline Builder**: Chain LLMs together for complex workflows
- **Session Management**: Save and resume conversations
- **Modern UI**: Built with Next.js 14, TypeScript, and Tailwind CSS

## Quick Start
1. `npm install`
2. `npx prisma db push`
3. `npm run dev`
4. Open [http://localhost:3000](http://localhost:3000)

â€¦  
SETUP_INSTRUCTIONS.txt
Multi-LLM Chat Platform - Setup Complete!
========================================

Your project has been created at: /tmp/multi-llm-chat-platform-<ID>

To complete the setup:

1. Navigate to the project directory.
2. Run: npm install
3. Run: npx prisma db push
4. Run: npm run dev
5. Open http://localhost:3000

Happy coding!
prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String?
  userId    String?
  messages  Message[]
  pipelines Pipeline[]
}

model Message {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  content    String
  role       String
  model      String
  tokens     Int?
  latency    Int?
}

model Pipeline {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  session    Session? @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId  String?
  name       String
  steps      String
  results    String?
  status     String   @default("pending")
}

model LLMProvider {
  id         String   @id @default(uuid())
  name       String   @unique
  endpoint   String
  apiKey     String
  models     String
  enabled    Boolean  @default(true)
  createdAt  DateTime @default(now())
}
src/lib/prisma.ts
import { PrismaClient } from '@prisma/client'
const globalForPrisma = globalThis as any
export const prisma = globalForPrisma.prisma ?? new PrismaClient()
if (process.env.NODE_ENV !== 'production') globalForPrisma.prisma = prisma
export default prisma
src/types/index.ts
export interface Message {
  id: string
  content: string
  role: 'user' | 'assistant' | 'system'
  model: string
  createdAt: Date
  tokens?: number
  latency?: number
}

export interface Session {
  id: string
  title?: string
  createdAt: Date
  updatedAt: Date
  messages: Message[]
}

export interface LLMProvider {
  id: string
  name: string
  endpoint: string
  models: string[]
  enabled: boolean
}

export interface PipelineStep {
  model: string
  prompt: string
  temperature?: number
  maxTokens?: number
}

export interface Pipeline {
  id: string
  name: string
  steps: PipelineStep[]
  status: 'pending' | 'running' | 'completed' | 'failed'
  results?: any
}
src/middleware.ts
import { NextResponse } from 'next/server'
import type { NextRequest } from 'next/server'

export function middleware(request: NextRequest) {
  const response = NextResponse.next()
  response.headers.set('Access-Control-Allow-Origin', '*')
  response.headers.set('Access-Control-Allow-Methods', 'GET, POST, PUT, DELETE, OPTIONS')
  response.headers.set('Access-Control-Allow-Headers', 'Content-Type, Authorization')
  return response
}

export const config = {
  matcher: '/api/:path*',
}
src/app/layout.tsx
import type { Metadata } from 'next'
import { Inter } from 'next/font/google'
import './globals.css'

const inter = Inter({ subsets: ['latin'] })

export const metadata: Metadata = {
  title: 'Multi-LLM Chat Platform',
  description: 'Orchestrate multiple LLMs in one platform',
}

export default function RootLayout({
  children,
}: {
  children: React.ReactNode
}) {
  return (
    <html lang="en" className="dark">
      <body className={inter.className}>{children}</body>
    </html>
  )
}
src/app/globals.css
@tailwind base;
@tailwind components;
@tailwind utilities;

:root {
  --foreground-rgb: 255, 255, 255;
  --background-start-rgb: 0, 0, 0;
  --background-end-rgb: 0, 0, 0;
}

body {
  color: rgb(var(--foreground-rgb));
  background: linear-gradient(
      to bottom,
      transparent,
      rgb(var(--background-end-rgb))
    )
    rgb(var(--background-start-rgb));
}

@layer base {
  * {
    @apply border-border;
  }
  body {
    @apply bg-background text-foreground;
  }
}
