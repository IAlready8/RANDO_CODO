    weight: 20
    flow:
      - get:
          url: "/health"
      - think: 1
  - name: "Single Task"
    weight: 40
    flow:
      - post:
          url: "/api/dispatch"
          json:
            role: "optimizer"
            payload: "Test content for optimization {{ $randomString() }}"
      - think: 2
  - name: "Chain Processing"
    weight: 30
    flow:
      - post:
          url: "/api/chain"
          json:
            threadId: "{{ $uuid() }}"
            payload: "Test content for chain processing {{ $randomString() }}"
            sequence: ["summarizer","optimizer","validator"]
      - think: 5
  - name: "System Info"
    weight: 10
    flow:
      - get:
          url: "/api/system"
