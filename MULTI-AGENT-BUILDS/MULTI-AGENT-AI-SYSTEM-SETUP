    "nginx:start": "sudo nginx -c $(pwd)/native-services/nginx/nginx.conf",
    "nginx:reload": "sudo nginx -s reload",
    "nginx:stop": "sudo nginx -s stop",
    "consul:start": "consul agent -dev -config-dir=native-services/consul/",
